{
    "manifest_version": 2,
    "name": "Spam Tweets Compressor",
    "short_name": "ST Compressor",
    "version": "1.1.4.0",
    "description": "Compress spam Tweets.",
    "icons": {
        "16": "image/icon16.png",
        "48": "image/icon48.png",
        "128": "image/icon128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://twitter.com/*"
            ],
            "js": [
                "dist/webextension_polyfill/browser-polyfill.min.js",
                "dist/main.js"
            ],
            "css": [
                "dist/main.css"
            ]
        }
    ],
    "browser_action": {
        "browser_style": true,
        "default_icon": {
            "16": "image/icon16.png",
            "48": "image/icon48.png",
            "128": "image/icon128.png"
        },
        "default_popup": "dist/browser_action.html",
        "default_title": "Spam Tweets Compressor"
    },
    "permissions": [
        "storage"
    ],
    "applications": {
        "gecko": {
            "id": "{feb13580-3a7d-4151-923b-2b39c1104fe4}"
        }
    }
}